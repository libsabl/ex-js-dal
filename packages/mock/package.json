{
  "name": "@appex/mock",
  "version": "0.1.0",
  "description": "Mocks and examples for example application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf dist coverage",
